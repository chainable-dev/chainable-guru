# OpenAI API - Get key from: https://platform.openai.com/account/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Supabase - Get credentials from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Vercel Blob Storage - Get token from Vercel project settings
BLOB_READ_WRITE_TOKEN=your_blob_storage_token

# Coinbase Developer Platform - Get from https://www.coinbase.com/developer-platform
NEXT_PUBLIC_CDP_API_KEY=your_cdp_api_key
NEXT_PUBLIC_CDP_API_KEY_NAME=organizations/your_org_id/apiKeys/your_key_id
NEXT_PUBLIC_CDP_API_KEY_PRIVATE_KEY=your_private_key

# Environment
NEXT_PUBLIC_ENVIRONMENT=localhost

# WalletConnect - Get from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# Vercel KV (Redis) - Get from Vercel KV dashboard
KV_URL=your_kv_url
KV_REST_API_URL=your_rest_api_url
KV_REST_API_TOKEN=your_api_token
KV_REST_API_READ_ONLY_TOKEN=your_readonly_token

# Base Network RPC URLs - Default public endpoints
NEXT_PUBLIC_BASE_MAINNET_RPC="https://mainnet.base.org"
NEXT_PUBLIC_BASE_SEPOLIA_RPC="https://sepolia.base.org"

# Chain IDs - Standard Base network chain IDs
NEXT_PUBLIC_BASE_MAINNET_CHAIN_ID="8453"
NEXT_PUBLIC_BASE_SEPOLIA_CHAIN_ID="84532"

# Contract Addresses - Standard USDC contract addresses on Base
NEXT_PUBLIC_USDC_MAINNET="0x833589fCD6eDb6E08f4c7C32D4f71b54bdA02913"
NEXT_PUBLIC_USDC_SEPOLIA="0x036CbD53842c5426634e7929541eC2318f3dCF7e"

# Rate Limiting Configuration
RATE_LIMIT_MAX_REQUESTS="100"
RATE_LIMIT_WINDOW_MS="60000"

# Debank API - Get from Debank dashboard
DEBANK_API_KEY=your_debank_api_key